{
  "name": "omni-email",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "docker:build": "docker build -t omni-email:latest .",
    "docker:build:prod": "docker build --target production -t omni-email:prod .",
    "docker:run": "docker run -p 3000:3000 omni-email:latest",
    "docker:run:prod": "docker run -p 3000:3000 omni-email:prod",
    "docker:compose:up": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "docker:compose:logs": "docker-compose logs -f",
    "docker:prune": "docker system prune -f"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "resend": "^6.0.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  }
}
